{   "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "root": true,
    "rules": {
        "semi": [
            2,
            "never"
        ],
        "comma-dangle": [
            2,
            "never"
        ],
        "arrow-parens": 0,
        "arrow-body-style": 0,
        "import/no-extraneous-dependencies": 0,
        "prefer-destructuring": 0,
        "prefer-template": 0,
        "no-useless-escape": 0,
        "global-require": 0,
        "consistent-return": 0,
        "no-underscore-dangle": 0
    },
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    }
}